{
  "name": "library/management",
  "type": "project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/er5bus/online-library-management.git"
  },
  "keywords": [
    "library",
    "management",
    "Book rental",
    "book management",
    "symfony"
  ],
  "author": "Rami Sfari",
  "description": "We want to make a complete Online Library solution that offers the standard functions of a library, namely searching for a book, borrowing a book, returning a book",
  "require": {
    "php": "^7.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "knplabs/knp-paginator-bundle": "^3.0",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "friendsofsymfony/user-bundle": "~2.0",
    "vich/uploader-bundle": "^1.9"
  },
  "require-dev": {
    "theofidry/alice-data-fixtures": "*",
    "hautelook/alice-bundle": "^2.5",
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "3.4.*"
    }
  }
}
